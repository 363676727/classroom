<ul>
<% @users.each do |u| %>
  <li><%= u.name %></li>
<% end %>
</ul>
<%= form_tag "/create_login_session" do %>
        <dl class="form">
          <dt><%= label_tag "name" %></dt>
          <dd><%= text_field_tag :name, params[:name] %></dd>
        </dl>
        <p> <%= submit_tag "login" %> </p>
<% end %>
